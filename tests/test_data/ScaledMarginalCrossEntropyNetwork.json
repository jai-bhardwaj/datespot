{
    "Version": 0.8,
    "Name": "ScaledMarginalCrossEntropyNetwork",
    "Type": "FeedForward",
    "ShuffleIndices": False,
    "ScaledMarginalCrossEntropy": {
        "positiveTarget": 1.0,
        "negativeTarget": 0.0,
        "positiveScale": 30.0,
        "negativeScale": 1.0
    },
    "Layers": [
        {
            "Name": "InputLayer",
            "Type": "Input",
            "Neurons": 2,
            "DataSet": "input",
            "Sparse": True
        },
        {
            "Name": "HiddenLayer",
            "Type": "Hidden",
            "SubType": "FullyConnected",
            "Source": "InputLayer",
            "Neurons": 2,
            "Activation": "Sigmoid",
            "Sparse": True
        },
        {
            "Name": "OutputLayer",
            "Type": "Output",
            "SubType": "FullyConnected",
            "DataSet": "output",
            "Neurons": 2,
            "Source": ["HiddenLayer"],
            "Activation": "Sigmoid",
            "Sparse": True
        }
    ],
    "ErrorFunction": "ScaledMarginalCrossEntropy"
}
