{
    "Version": 0.8,
    "Name": "Linear Regression Model",
    "Type": "FeedForward",
    "ShuffleIndices": false,
    "Layers": [
        { 
            "Name": "InputLayer",
            "Type": "Input",
            "N": 1,
            "DataSet": "input_data",
